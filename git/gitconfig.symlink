[user]
	name = Mark Mossberg
	email = mark.mossberg@gmail.com
[color]
	ui = true
[push]
	default = nothing
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore
[alias]
	s = "status -sb"
    d = "diff"
	st = "status"
	a = "add"
	au = "add -u"
	c = "commit"
	l = "log"
	ll = "log --graph --oneline --all --decorate -n 25"
	lll = "log --graph --oneline --all --decorate"
	p = "push"
	co = "checkout"
	b = "branch"
