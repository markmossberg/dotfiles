[user]
	name = Mark Mossberg
	email = mark.mossberg@gmail.com
[credential]
	helper = osxkeychain
[color]
	ui = true
[push]
	default = simple
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/mark/.gitignore
[alias]
	s = "status -sb"
	st = "status"
	a = "add"
	c = "commit"
	l = "log"
	ll = "log --graph --oneline --all --decorate"
	p = "push"
	co = "checkout"
	b = "branch"
